{
  "name": "travelexplore-website",
  "version": "1.0.0",
  "description": "A fully functional responsive travel tour website with booking system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build -t travelexplore .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f app",
    "db:seed": "node scripts/seed-database.js",
    "db:backup": "node scripts/backup-database.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "prod": "NODE_ENV=production node server.js"
  },
  "keywords": [
    "travel",
    "tourism",
    "booking",
    "website",
    "express",
    "nodejs"
  ],
  "author": "TravelExplore Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.0",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/travelexplore-website.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/travelexplore-website/issues"
  },
  "homepage": "https://github.com/your-username/travelexplore-website#readme"
}
